<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />

    <!-- Load React. -->
    <!-- Note: when deploying, replace "development.js" with "production.min.js". -->
    <script
      src="https://unpkg.com/react@18/umd/react.development.js"
      crossorigin
    ></script>
    <script
      src="https://unpkg.com/react-dom@18/umd/react-dom.development.js"
      crossorigin
    ></script>

    <!-- material-ui CDN -->
    <script
      src="https://unpkg.com/@mui/material@latest/umd/material-ui.development.js"
      crossorigin="anonymous"
    ></script>

    <!-- load howler for audio -->
    <script src="https://cdn.jsdelivr.net/npm/howler@2.2.3/dist/howler.min.js"></script>

    <!-- load papa parse to parse bookmark csv -->
    <!-- <script src='https://cdnjs.cloudflare.com/ajax/libs/PapaParse/5.3.2/papaparse.min.js'></script> -->


    <!-- Load Bable -->
    <!-- order is important...BABEL should come at last -->
    <script src="https://unpkg.com/babel-standalone@6/babel.min.js"></script>
  </head>
  <body>
    <div id="root"></div>
    <script type="text/babel">
      const { Slider,Select,FormControl,InputLabel,MenuItem } = MaterialUI;
    </script>
    <!-- APP component -->
    <script
      type="text/babel"
      src="javascripts/REACT/Components/ProgressBar.js"
    ></script>
    <script
      type="text/babel"
      src="javascripts/REACT/Components/PlaybackControl.js"
    ></script>
    <script
      type="text/babel"
      src="javascripts/REACT/Components/BookmarkCreator.js"
    ></script>
    <script
      type="text/babel"
      src="javascripts/REACT/Components/BookmarkList.js"
    ></script>
    <script
      type="text/babel"
      src="javascripts/REACT/Components/SelectAudio.js"
    ></script>
    <script
      type="text/babel"
      src="javascripts/REACT/Components/PlayAudio.js"
    ></script>
    <script
      type="text/babel"
      src="javascripts/REACT/Components/Container.js"
    ></script>
    <!-- Index component -->
    <script type="text/babel" src="/javascripts/REACT/index.js"></script>
  </body>
</html>
